<migrate-config>
	<source type="jdbc">
		<data-source>
			<driver-class value="${source.jdbc.driverClass}"/>
			<url value="${source.jdbc.jdbcUrl}"/>
			<username value="${source.jdbc.user}"/>
			<password value="${source.jdbc.password}"/>
		</data-source>
		<dialect type="org.yubing.datmv.type.jdbc.dialect.MySqlDialect"/>
		<table-name value="user"/>
	</source>
	
	<target type="jdbc">
		<data-source>
			<driver-class value="${target.jdbc.driverClass}"/>
			<url value="${target.jdbc.jdbcUrl}"/>
			<username value="${target.jdbc.user}"/>
			<password value="${target.jdbc.password}"/>
		</data-source>
		<dialect type="org.yubing.datmv.type.jdbc.dialect.MySqlDialect"/>
		<table-name value="user"/>
	</target>
	
	<config-items>
		<item name="id" type="int" mapping-key="id"/>
		
		<item name="phone_num" type="string" mapping-key="phone_num"/>
		<item name="password" type="string" mapping-key="password"/>
		
		<item name="email" type="string" value=""/>
		<item name="family_name" type="string" mapping-key="last_name"/>
		<item name="given_name" type="string" mapping-key="first_name"/>
		
		<item name="gender" type="int" value="-1"/>
		<item name="birthday" type="date"/>
		<item name="is_luna_birthday" type="bool" value="0"/>
		<item name="status" type="int" value="1"/>
		<item name="roles" type="int" value="1"/>
		<item name="ts_edit" type="date"/>
		<item name="ts_create" type="date"/>
	</config-items>
	
	<listener value="log"/>
			
</migrate-config>